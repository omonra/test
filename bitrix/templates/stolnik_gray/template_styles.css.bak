html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
    margin: 0; padding: 0; outline: 0; vertical-align: baseline; background: transparent;}

ul, li {list-style-type:none;}

h1, h2, h3, h4, h5, h6 {font-weight: normal;}

img, fieldset {border: 0px;}

.ie6 img.png {behavior: url(style/iepngfix.htc);}


a {color: #000000; text-decoration: underline;}
a:hover {color: #cc1714; text-decoration: none;}


sup {vertical-align: super; font-size: 9px; line-height: 4px;}

table {border: 0px; border-collapse: collapse;}
td {padding: 0px;}



/* elements */

input { font-family: 'Open Sans', sans-serif; font-size: 12px; color: #333;}
.text_input { height: 16px; margin: 0px; padding: 2px 4px; border: 1px solid #ababb3; border-color: #ababb3 #e4e3e9 #e4e9ef; vertical-align: top;}
textarea {resize: none;  margin: 0px; padding: 2px 4px; border: 1px solid #ababb3; border-color: #ababb3 #e4e3e9 #e4e9ef; font-family: 'Open Sans', sans-serif; font-size: 12px; color: #333;}
select {height: 22px; margin: 0px; border: 1px solid #ababb3; border-color: #ababb3 #e4e3e9 #e4e9ef; font-family: 'Open Sans', sans-serif; font-size: 14px; color: #333;}




/* checkbos & radio */

.checkbox,
.radiobox {position: relative; width: 14px; height: 14px; margin: 0px; padding: 0px; outline: none; vertical-align: top;}

.c_checkbox,
.c_radio {position: relative; zoom :1; display: inline-block; width: 17px; height: 15px; background: url("images/bg_check.png") no-repeat left top;}
.c_checked {background-position: left -15px;}



/* text input */

.text_input_1 {
    position: relative; height: 18px; padding: 5px 18px; border: 1px solid #999; color: #cc1714; font-size: 14px;
    /* 	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; */
}
.text_input_1_2 {
    position: relative; height: 18px; padding: 5px 18px; border: 1px solid #999; color: #cc1714; font-size: 14px; width:200px;
    /* 	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; */
}
.text_input_1_err {
    border-color: #cc1714;
    -moz-box-shadow: 0 0 4px rgba(204, 23, 40, 0.3); -webkit-box-shadow: 0 0 4px rgba(204, 23, 40, 0.3); box-shadow: 0 0 4px rgba(204, 23, 40, 0.3);
}


.text_input_2 {
    position: relative; height: 16px; padding: 1px 10px 1px; border: 1px solid #999; background: #ebebeb; vertical-align: top; color: #4c4c4c; font-size: 14px;
    -moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;
}


.text_input_3 {
    position: relative; height: 16px; padding: 2px 10px 2px; border: 0px; background: #fff; vertical-align: top; color: #4c4c4c; font-size: 14px;
    -moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;
}



.input_value_ok .holder {display: none;}


.textarea_1 {
    position: relative; display: block; width: 453px; height: 140px; padding: 4px 6px 4px 19px; border: 1px solid #999; font-size: 14px; color: #cc1714; line-height: 20px;
    /* 	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; */
}
.text_input_1_err {
    border-color: #cc1714;
    -moz-box-shadow: 0 0 4px rgba(204, 23, 40, 0.3); -webkit-box-shadow: 0 0 4px rgba(204, 23, 40, 0.3); box-shadow: 0 0 4px rgba(204, 23, 40, 0.3);
}

/* select */
.custom_select {position: relative; zoom: 1; overflow: hidden; display: block; width: 187px; height: 30px; background: #fff; vertical-align: top; text-align: left;}
/* .ie6-7 .custom_select {display: inline; } */
.custom_select .select_in {position: relative; zoom: 1; display: block; height: 23px; padding: 5px 26px 0 14px; border: 1px solid #999; background: #fff; font-size: 14px; line-height: 15px; color: #4e4e4e; font-family: 'Open Sans', sans-serif;}
.custom_select .select_in span {position: relative; display: block; overflow: hidden; width: 100%; white-space: nowrap; }
.custom_select .select_in a {position: absolute; zoom: 1; z-index: 2; top: 0px; left: 0px; width: 100%; height: 28px; background: transparent url("images/transparent.gif");}
.custom_select .select_in a i {position: absolute; top: 11px; right: 8px; width: 11px; height: 6px; background: transparent url("images/icon_arrow_1_down_2.gif") no-repeat left top; font-size: 0px;}
.ie6-7 .custom_select .select_in a {width: expression(offsetParent.offsetWidth - 2 + 'px');}

.custom_select ul {display: none;}

#c_form_select_wrap {position: absolute; z-index: 2000; display: none;}
#c_form_select_wrap ul {position: relative; zoom: 1; max-height: 300px; overflow: auto; overflow-x: hidden; border: 1px solid #ccc; padding: 0px 0; background: #fff; list-style-type: none;}
.ie6 #c_form_select_wrap ul {height: expression(this.scrollHeight > 299 ? "300px" : "auto");}
#c_form_select_wrap ul li {position: relative; zoom: 1; display: block; vertical-align: top; list-style-position: outside;}
#c_form_select_wrap ul a {position: relative; display: block; padding: 4px 9px 5px; text-decoration: none; color: #000; vertical-align: top;}
#c_form_select_wrap ul a:hover {background: #ededed; text-decoration: none;}
#c_form_select_wrap ul a.selected {background: #e3e3e3;}

#c_form_select_wrap.c_form_select_wrap_active {display: block;}



/* buttons */

.button_1 {position: relative; zoom: 1; overflow: visible; display: inline-block; height: 30px; padding: 0 40px; border: 0px; background: #40434d; vertical-align: top; color: #fff; font-size: 14px; line-height: 30px; cursor: pointer; text-align: center; text-decoration:none;}
.button_1:hover {background:#f00; color:#fff;}
.gray {background:#d9d9db;}

.button_1_1 {background: #ebebeb; color: #4e4e4e;}

.button_1_2 {background: #40434d; color: #fff; line-height: 37px; text-transform:uppercase;}
.button_1_2:hover {background: #ff0000; color: #fff; text-decoration:none;}
a.button_1_2 {height: 37px; text-decoration:none;}
input.button_1_2 {height: 50px; text-decoration:none;}


.form_hidden {position: absolute !important; left: -999999px;}



/* icons */

.icon_product_hit {position: relative; zoom: 1; display: inline-block; width: 90px; height: 90px; background: transparent url("images/icon_product_hit.png") no-repeat left top; vertical-align: top; font-size: 0px;}
.icon_product_new {position: relative; zoom: 1; display: inline-block; width: 90px; height: 90px; background: transparent url("images/icon_product_new.png") no-repeat left top; vertical-align: top; font-size: 0px;}
.icon_product_sale {position: relative; zoom: 1; display: inline-block; width: 90px; height: 90px; background: transparent url("images/icon_product_sale.png") no-repeat left top; vertical-align: top; font-size: 0px;}






/* BASE
----------------------------------------------------------------------------- */

html { height: 100%;}
body {position: relative; width: 100%; height: 100%; background: #fff; font-family: 'Open Sans', sans-serif; font-size: 14px; line-height: 18px; color: #000000;}


.wrapper {position: relative; zoom: 1; min-width: 1000px; min-height: 100%;}
.ie6 .wrapper {height: 100%; width: expression((document.documentElement.clientWidth || document.body.clientWidth) < 999? "1000px":"auto");}



/* header */

.header {position: relative; zoom: 1; margin: 0 0 35px; z-index: 40;}
.header .header_in {position: relative; zoom: 1; width: 1000px; margin: 0 auto 20px; padding: 7px 0 12px;}
.header .header_in:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}

.header .phone {position: relative; float: left; background: url("images/header/phone.png") no-repeat left top; padding-left:15px; font-size:13px;}
.header .phone span {cursor:pointer; font-size:12px;}
.header .phone span:hover {color:#f00;}
.header .phone p {margin-top: -3px;}
.header .logo {position: relative; float: left; margin: 0 26px 0 200px;}
.header .search_block {position: relative; float: left; padding: 0 0 10px 10px; background: transparent url("images/bg_search_block.gif") no-repeat left bottom;}
.header .search_block .block_in {position: relative; zoom: 1; overflow: hidden; width: 478px; height: 38px; border: 1px solid #4e4e4e;}
.header .search_block .text_input {position: relative; display: inline-block; float: left; height: 38px; padding: 0px; border: 0px; vertical-align: top;}
.header .search_block .text_input input {position: relative; width: 365px; height: 16px; padding: 11px 7px; border: 0px; outline: none; background: transparent; vertical-align: top; font-size: 14px; color: #4e4e4e;}
.header .search_block .text_input .holder {position: absolute; z-index: 2; top: 10px; left: 8px; font-size: 14px; color: #4e4e4e;}
.header .search_block .submit {position: relative; width: 99px; height: 38px; padding: 6px 0 7px; border: 0px; background: #cc1714; outline: none; text-align: center; font-size: 18px; color: #fff; font-family: 'Open Sans', sans-serif; text-transform: uppercase; cursor: pointer;}

.header .basket_block {position: relative; float: right; width: 144px; margin: -11px 0 0; background: transparent url("images/bg-cart.png") no-repeat left;padding-left: 27px; font-size:11px; height: 59px;}
/*.header .basket_block dl {position: relative; zoom: 1; overflow: hidden; margin: 0 0 17px; font-family: 'Open Sans', sans-serif; text-transform: uppercase; font-weight: bold; line-height: 30px;}*/
.header .basket_block dl {position: relative; zoom: 1; overflow: hidden; margin: 15px 0 0; font-family: 'Open Sans', sans-serif; text-transform: uppercase; font-weight: bold;}
.header .basket_block dt {position: relative; float: left; clear: left; padding: 0 5px 0 0;}
.header .basket_block dd {position: relative; zoom: 1; overflow: hidden; }
.header .basket_block dd .val {position: relative; top: -2px; font-size: 29px; vertical-align: middle; }
.header .basket_block .summ {color: #cc1714;}
.header .basket_block .order_link {color: #cc1714;}


.header .cat {position: relative; zoom: 1; color: #000; font-family: 'Open Sans', sans-serif; text-transform: uppercase; font-size: 13px; line-height: 30px; font-weight: bold;}
.header .cat .menu_cat {width:980px; margin: 0 auto; border-bottom: 3px solid #d9d9db;}
.header .cat_in {position: relative; zoom: 1; width: 500px; margin: 0 auto; padding: 3px 1px 6px 19px; list-style-type: none;}
.header .cat_in:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.header .cat .item {position: relative; float: left; margin: 0 29px;}
.header .cat .item_in {position: relative; zoom: 1;}
.header .cat .item_h {position: relative; zoom: 1;}
.header .cat .separate {position: absolute; top: 13px; left: -31px; display: block; width: 11px; height: 5px; background: transparent url("images/marker_4.gif") no-repeat left top; font-size: 0px;}
.header .cat a {color: #000; text-decoration: none;}

.header .cat .item_active .item_h a {color: #cc1714;}
.header .cat .item_in:hover .sub {display: block;}

.header .cat .sub {position: absolute; display: none; top: 26px; left: 0px; width: 502px; padding: 10px 0 0; font-family: 'Open Sans', sans-serif; font-size: 14px; line-height: 20px; font-weight: normal; }
.header .cat .sub .block_out {position: relative; top: 11px; left: -9px;}
.header .cat .sub .block_in {position: relative; zoom: 1; top: -11px; left: 9px; border: 1px solid #d9d9db; background: #fff;}
.header .cat .sub .block_corner {position: absolute; zoom: 1; top: -11px; left: 20px; width: 22px; height: 11px; background: transparent url("images/bg_cat_sub.gif") no-repeat left top; font-size: 0px;}
.header .cat .sub .block_h {position: relative; zoom: 1; padding: 15px 20px 5px; background: #e6e6e6; font-size: 14px; font-weight: bold; font-family: 'Open Sans', sans-serif; color: #000; }
.header .cat .sub .block_content {position: relative; zoom: 1; padding: 15px 20px 10px; text-transform: none;}
.header .cat .sub .block_content:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.header .cat .sub .block_content ul {position: relative; zoom: 1; float: left; list-style-type: none;}
.header .cat .sub .block_content ul.list_1 {width: 202px; margin: 0 -1px 0 0; padding: 0 10px 0 0; background: transparent url("images/line_vert_1.gif") repeat-y right top; }
.header .cat .sub .block_content ul.list_2 {width: 220px; padding: 0 0 0 18px; background: transparent url("images/line_vert_1.gif") repeat-y left top; }
.header .cat .sub .block_content ul li {position: relative; zoom: 1; vertical-align: top;}
.header .cat .sub .block_content a {color: #000;}
.header .cat .sub .block_content a:hover {text-decoration: underline;}
.header .cat .sub .block_content .active a {color: #cc1714; text-decoration: underline;}
.header .cat .sub .block_content .active a:hover {text-decoration: none;}



.header .auth {position: relative; zoom: 1; background: url("images/header/head-bg.png") repeat-x; height: 80px;}
.header .auth_in {position: relative; zoom: 1; overflow: hidden; width: 1000px; margin: 0 auto; }

.header .auth .toplinemenu {position: relative; zoom: 1; float: left; margin:12px 0 0; padding: 0; text-align: right; font-family: 'Open Sans', sans-serif; color: #40434d; font-size: 11px; font-weight:bold; line-height: 10px; }
.header .auth .toplinemenu li.shops {padding-left:15px; background: url("images/header/shop-ico.png") no-repeat left 0;}
.header .auth .toplinemenu li.shops:hover {background-position: left -11px;}
.header .auth .toplinemenu li a {color: #40434d; text-decoration:none;}
.header .auth .toplinemenu li a:hover {color: #f00;}
.header .auth ul {position: relative; zoom: 1; float: right; margin: 10px -5px 0 0; padding: 0; text-align: right; text-transform: uppercase; font-family: 'Open Sans', sans-serif; color: #40434d; font-size: 11px; font-weight:bold; line-height: 10px; white-space: nowrap;}
.ie6-7 .header .auth ul { line-height: 14px;}
.header .auth ul li {position: relative; zoom: 1; display: inline-block; padding: 0 8px 0 8px;}
.ie6-7 .header .auth ul li {display: inline;}
.header .auth ul a {color: #40434d;}
.header .auth ul .icon {position: relative; margin: 0 0 0 4px;}


.header .auth dl {position: relative; zoom: 1; float: right; margin: 0; padding: 12px 0 15px; text-align: right; text-transform: uppercase; font-family: 'Open Sans', sans-serif; color: #40434d; font-weight:bold; font-size: 11px; line-height: 10px;}
.ie6-7 .header .auth dl { line-height: 14px;}
.header .auth dl dt {position: relative; zoom: 1; display: inline-block; padding: 0 8px 0 0; vertical-align: top;}
.ie6-7 .header .auth dl dt {display: inline;}
.header .auth dl dd {position: relative; zoom: 1; display: inline-block; padding: 1px 6px 0px 8px; vertical-align: top;}
.ie6-7 .header .auth dl dd {display: inline;}
.header .auth dl .search {padding-left:30px; border-left:1px solid #999; margin-left:15px;background: url("images/header/search-act.png") no-repeat 10px top;}
.header .auth dl .search:hover {background-position: 10px -10px;}
.header .auth dl a,
.header .auth dl span {color: #40434d; text-decoration: none; cursor:pointer;}
.header .auth dl a:hover,
.header .auth dl span:hover {color: #cc1714;}

.header .auth .login {
	padding-left: 15px;
	background: url("images/header/login.png") no-repeat left 1px;
}
.header .auth .logout {
	padding-right: 15px;
	background: url("images/header/login.png") no-repeat right 1px;
}
.header .auth .login:hover {
	background-position: left -10px;
}
.header .auth .logout:hover {
	background-position: right -10px;
}

/* footer */

.footer {position: relative; zoom: 1; min-width: 1000px; height: 336px; margin: -338px 0 0;}
.footer .footer_in {position: relative; zoom: 1; width: 980px; margin: 0 auto; padding: 20px 0 0 20px;}
.footer .footer_in:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}


.footer .block_menu {position: relative; zoom: 1; height: 239px; padding: 14px 0 0;}

.footer .footer_menu {position: relative; zoom: 1; margin: 0 0 35px -20px; padding-top:20px; font-size: 12px; line-height: 20px; border-top: 3px solid #d9d9db;}
.footer .footer_menu:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.footer .footer_menu dl {position: relative; float: left; width: 140px; padding: 0 0 0 20px;}
.footer .footer_menu dt {position: relative; zoom: 1; margin: 0 0 10px; font-family: 'Open Sans', sans-serif; font-size:13px; font-weight: bold; text-transform: uppercase;}
.footer .footer_menu dd {position: relative; zoom: 1;}
.footer .footer_menu .social dl:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.footer .footer_menu .social dd { overflow: hidden; padding: 0 12px 0 20px; font-weight: bold; float:left;}
.footer .footer_menu .social dd .icon {position: relative; float: left; margin: 2px 0 0 -20px;}
.footer .footer_menu .social dd a {}
.footer .footer_menu .pay_list:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.footer .footer_menu .pay_list {width:335px;}
.footer .footer_menu .pay_list dd {position: relative; zoom: 1; float:left; padding:0 4px;}
.footer .footer_menu a {}

.footer .pay_list {position: relative; zoom: 1; list-style-type: none;}
.footer .pay_list:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.footer .pay_list li {position: relative; float: left; margin: 0 17px 0 0; vertical-align: top;}



.footer .block_info {position: relative; zoom: 1; padding: 13px 0 0; font-size: 13px; line-height: 21px;}
.footer .block_info .info {position: relative; width: 730px; float: left;}
.footer .block_info .develop {position: relative; width: 230px; float: right; text-align: right;}
.footer .block_info .color {color: #cc1714;}




/* content */

.content {position: relative; zoom: 1; width: 980px; margin: 0 auto; padding: 0 0 355px 5px;}
.content:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.content .sidebar {position: relative; float: left; width: 230px; margin: 0 20px 0 0;}
.content .sidebar form {padding:0 10px; background: #f4f4f4;}
.content .base_content {position: relative; zoom: 1; float: right; width: 730px; padding: 0px;}
.content .in_card {width: 1000px;}

.base_content_only .base_content {float: none; width: auto;}

.city_shop{border-bottom:1px solid #d9d9db;}
.city_shop caption{text-align:left; text-transform:uppercase; font-size: 20px; margin: 30px 0 10px;}
.city_shop th{font-weight:normal; padding: 10px 5px; background:#f7f7f7;}
.city_shop tr{border-top:1px solid #d9d9db;}
.city_shop tr:hover{background:#f7f7f7;}
.shop_address{width:550px;}
.shop_phone, .shop_time{border-left:1px solid #d9d9db; width:200px; text-align:center;}
.shop_address,
.shop_phone,
.shop_time{padding:10px 5px;}


/* CONTENT BLOCK'S
----------------------------------------------------------------------------- */

/* HOME */

.home_reasons{margin: 0 auto;position: relative;width: 1000px; background: url("images/header/paling-top.png") repeat-x left top;}
.home_reasons:after {clear: both;content: " ";display: block;font-size: 0;height: 0;overflow: hidden;width: 0;}
.home_reasons h2{font-size:21px!important; font-weight:bold!important; text-align:center; margin:0; padding:20px 0; background: url("images/header/paling-bottom.png") repeat-x left bottom;}
.home_reason{float:left;width:475px;margin:5px 0 15px;padding: 0 0 0 25px;}
.home_reason h3{font-size:13px; font-weight:bold; margin:0 0 10px;}
.home_reason p {font-size:12px;}
.reason-1{background: url("images/numbers/1.png") no-repeat left top;}
.reason-2{background: url("images/numbers/2.png") no-repeat left top;}
.reason-3{background: url("images/numbers/3.png") no-repeat left top;}
.reason-4{background: url("images/numbers/4.png") no-repeat left top;}
.reason-5{background: url("images/numbers/5.png") no-repeat left top;}
.reason-6{background: url("images/numbers/6.png") no-repeat left top;}


/* side block's 
----------------------------------------------------------------------------- */


.side_block {position: relative; zoom: 1; margin: 0px 0 14px; padding: 16px 0 0; border-top:dotted 1px black; }
.side_block:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.side_block h2 {position: relative; zoom: 1; margin: 0 0 12px;  font-family: 'Open Sans', sans-serif; font-size: 14px; line-height: 18px; text-transform: uppercase; text-align: center;}

.side_block_first { border:none;}



/* menu */
.side_block_menu {background: transparent url("images/bg_side_block_1.gif") no-repeat center top; }

.side_menu {position: relative; zoom: 1; list-style-type: none; line-height: 20px;}
.side_menu li {position: relative; zoom: 1; padding: 0 0 0 18px; background: transparent url("images/marker_1.gif") no-repeat left 11px; vertical-align: top;}
.side_menu a {color: #4e4e4e;}
.side_menu li.active {background: transparent url("images/marker_2.gif") no-repeat left 11px; font-weight: bold;}
.side_menu li.active a {color: #992d2c; text-decoration: underline;}
.side_menu li.active a:hover {text-decoration: none;}
.side_menu .icon_new {position: relative; margin: 5px 0 0 6px; vertical-align: top;}




/* new */
.side_block_new {min-height: 32px; padding-left: 18px; background: transparent url("images/icon_exclamation.gif") no-repeat 2px 5px; color: #cc1714; line-height: 20px;}


/* action */
.side_block_action { background: transparent url("images/bg_side_block_2.gif") no-repeat center top; }
.side_block_action h2 {color: #992d2c;}

.side_block_action .side_menu li { background: transparent url("images/marker_3.gif") no-repeat left 11px;}
.side_block_action .side_menu a {color: #992d2c;}



/* good to know */
.side_block_good_to_know { /*background: transparent url("images/bg_side_block_3.gif") no-repeat center top;*/ }




/* filter */
.side_block_filter h2 {padding: 1px 0 0; text-align: left;}
.side_block_filter .clear {position: absolute; display: none; top: 14px; right: 0px; color: #999; font-size: 11px;}

.side_block_filter.side_block_first .clear { top: 0px; }
.side_block_filter_checked .clear { display: block; }


.side_block_filter .filter_list {position: relative; zoom: 1; list-style-type: none;}
.side_block_filter .filter_list li {position: relative; zoom: 1; margin: 0 0 2px; padding: 0 0 0 18px;}
.side_block_filter .filter_list .radio,
.side_block_filter .filter_list .checkbox {position: relative; float: left; margin: 3px 0 0 -18px;}
.side_block_filter .filter_list .c_radio,
.side_block_filter .filter_list .c_checkbox {position: relative; float: left; margin: 0 0 0 -15px; cursor:pointer;}

