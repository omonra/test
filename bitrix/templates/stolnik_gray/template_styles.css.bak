/* img pagination */
#main{position:relative;}
#holder{width:63px; overflow:hidden; position:relative;}
.swControls{position:absolute; margin-top: 20px;}
a.swShowPage{
	/* The links that initiate the page slide */
	background-color:#fff;
	float:left;
	height:12px;
	margin:4px 3px;
	text-indent:-9999px;
	width:12px;
	border:3px solid #d9d9db;
	
	/* CSS3 rounded corners */
	
	-moz-border-radius:7px;
	-webkit-border-radius:7px;
	border-radius:7px;
}
a.swShowPage:hover, a.swShowPage.active{background-color:#ff0000; border:3px solid #ff0000;}
#holder li{display: block;}
.swPage{float:left;}
.clear{clear:both;}

