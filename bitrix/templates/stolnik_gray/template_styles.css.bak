.faq_delivery h3 {background: url("images/faq/truck.png") no-repeat left top;}
.faq_return h3 {background: url("images/faq/arrow_left.png") no-repeat left top;}
.faq_item {float:left;width:300px;margin:10px; font-size:13px;padding-bottom:15px; border-bottom:1px dotted #40434d;}
.faq_item h3 {font-weight:bold; padding-bottom:15px;cursor:pointer;}
.faq_container:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}

#faq_feedack {}
.mf-text {float:left; width:100px;}

/* basket */

.basket_block_page {position: relative; zoom: 1; }
.basket_block_page h1 { margin-bottom: 24px; text-align:center;}

.basket_block_page .deferred_products {position: relative; zoom: 1; margin: 30px 0 0;}
.basket_block_page .deferred_products h4 {text-align:center; text-transform:uppercase; margin-bottom:30px; font-weight:bold;}

.basket_block_page .ordering_options .submit {width: 210px; float: right; margin: 0 0 0 40px; padding: 0px; }
.basket_block_page .ordering_options .update {width: 100px; float: right; margin: 0 0 0 0px; padding: 0px; }

.basket_block_page .deferred_products .update {position: relative; float: right; width: 100px; margin: 0 250px 0 0px; padding: 0px; }
.basket_block_page .deferred_products .basket_items td {color: #a9a9a9;}


.basket_items {margin: 0 0 20px;}
.basket_items td {padding-bottom: 10px; vertical-align:middle;}
.basket_items .price {width: 110px;}
.basket_items .count {width: 120px; text-align:center;}
.basket_items .count .text_input_3 {width: 40px; padding-left: 0px; padding-right: 0px; text-align: center; border:1px solid #d9d9db;}
.basket_items .defer {width: 120px; text-align: center; color: #999;}
.basket_items .del {width: 30px; text-align: center; color: #cc1714;}

.basket_items .c_radio,
.basket_items .c_checkbox {
    position: relative; width: 20px; height: 20px; margin: -1px 0; border: 1px solid #d9d9db; background: #fff;
    -moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;
}
.basket_items .del .c_radio,
.basket_items .del .c_checkbox {
    position: relative; width: 20px; height: 20px; margin: -1px 0; border: 0px; background: transparent url("images/basket/x.png") no-repeat left top;
}
.basket_items .c_checked {background: transparent url("images/bg_check_2.gif") no-repeat left top;}
.basket_items .del .c_checked {background: transparent url("images/basket/xb.png") no-repeat left top;}













/* autocomplite
----------------------------------------------------------------------------- */


.ac_results {
    padding: 0px;
    border: 1px solid WindowFrame;
    background-color: Window;
    overflow: hidden;
}

.ac_results ul {
    width: 100%;
    list-style-position: outside;
    list-style: none;
    padding: 0;
    margin: 0;
}

.ac_results iframe {
    display:none;/*sorry for IE5*/
    display/**/:block;/*sorry for IE5*/
    position:absolute;
    top:0;
    left:0;
    z-index:-1;
    filter:mask();
    width:3000px;
    height:3000px;
}

.ac_results li {
    position:relative;
    margin: 0px;
    padding: 2px 5px;
    cursor: pointer;
    display: block;
    width: 100%;
    font: menu;
    font-size: 12px;
    overflow: hidden;
}

.ac_loading {
    background : Window url('autocomplete_indicator.gif') right center no-repeat;
}

.ac_over {
    background-color: Highlight;
    color: HighlightText;
}


.arrow {
    position: absolute;
    z-index: 9999;

    top: 87%;
    cursor: pointer;
}

.arrow a {
    display: block;
    height: 37px;
    width: 22px;
}

.arrow-left {
    left: 3%;
    background: url("images/img_slider_arrows.png") no-repeat ;
}

.arrow-right {
    left: 87%;
    background: url("images/img_slider_arrows.png") no-repeat -22px 0px;
}



/* clood zoom */

.cloud-zoom {position: relative; display: block; cursor: move;}
.cloud-zoom img {position: relative; display: block; /*border: 3px solid #ececec;*/ cursor: move;}


.cloud-zoom-lens {margin: -2px; border: 2px solid #dbdbdb; background: #fff; cursor: move;}
.cloud-zoom-big {
    position: absolute; zoom: 1; overflow: hidden; z-index: 999; /*margin: 50px 0 0 -880px !important;*/ border: 5px solid #fff;
    -moz-box-shadow: 1px 1px 3px #555; -webkit-box-shadow: 1px 1px 3px #555; box-shadow: 1px 1px 3px #555;
}
.cloud-iframe {margin: 50px 0 0 -880px !important;}









/* POPUP
----------------------------------------------------------------------------- */

.popup {position: relative; zoom: 1; width: 480px; }
.popup .popup_in {position: relative; zoom: 1; padding: 23px 19px 20px 39px;}
.popup h1 {position: relative; zoom: 1; margin: 0 0 10px; font-family: 'Open Sans', sans-serif; color: #000; font-weight: bold; font-size: 29px; line-height: 32px; text-transform: uppercase;}


.popup_item_in_basket .mess {position: relative; zoom: 1; margin: 0 0 16px; font-family: 'Open Sans', sans-serif; color: #4e4e4e; font-weight: bold; font-size: 14px; line-height: 18px; text-transform: uppercase;}
.popup_item_in_basket .options {position: relative; zoom: 1; margin: 0 0 0 -20px; list-style-type: none;}
.popup_item_in_basket .options:after {content: ' '; clear: both; display: block; width: 0; height: 0; overflow: hidden; font-size: 0;}
.popup_item_in_basket .options li {position: relative; zoom: 1; float: left; padding: 0 0 0 20px; }
.popup_item_in_basket .options .button {padding: 0px; width: 200px;}

.size-popup {
	position: absolute;
	left: -50%; top: 0; 
	z-index: 10;
	background: rgba(255,255,255,0.9);
	padding: 15px 20px 5px;
	box-shadow: 0px 2px 15px 3px #777777;
	-webkit-box-shadow: 0px 2px 15px 3px #777777;
	-khtml-box-shadow: 0px 2px 15px 3px #777777;
	-moz-box-shadow: 0px 2px 15px 3px #777777;
}
.popup-base {
	position: absolute;
	left: 50%; top: 25%;
	margin-left:-300px; 
	z-index: 111;
	background: rgba(255,255,255,0.9);
	padding: 15px 20px;
	box-shadow: 0px 2px 15px 3px #777777;
	-webkit-box-shadow: 0px 2px 15px 3px #777777;
	-khtml-box-shadow: 0px 2px 15px 3px #777777;
	-moz-box-shadow: 0px 2px 15px 3px #777777;
}
.size-popup .close,
.popup-base .close {
	text-align:right;
	background: url("images/popup/close.png") no-repeat right 0;
	cursor:pointer;
	height:18px;
}
.size-popup .close:hover,
.popup-base .close:hover {
	background-position: right -18px;
}
.popup-base .form_list dd.captcha {margin-left: -40px;}
.call .form_list dd.captcha input {width:200px;}
.call .form_list textarea {width:200px; height:100px; border:1px solid #999999;padding: 5px 18px;}
.call .submit input {float:right; width:200px;}
.popup-base .reg_block {padding: 0 10px;}


/*  EXCLUSIVE
----------------------------------------------------------------------------*/
.exclusive{
    width:213px;
    height:20px;
    background:black;
    color:white;
    text-align:center;
    font-size:14px;
