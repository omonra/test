    <script type="text/javascript">
        $(window).scroll(function () {
            //$(".go_top").css("display", "inline").fadeOut("hide");
            //$(".go_top").css("top",$(document).scrollTop()+'px')
            if($(document).scrollTop()>200)
                $(".go_top").show('300');
            else
                $(".go_top").hide('300');



            //console.log($(document).scrollTop())
        });

        $(document).ready(function(){
            $(".go_top").bind('mouseover',function(){
                $(".go_top").animate({opacity:'1.0'},600);
            })
            $(".go_top").bind('mouseout',function(){
                $(".go_top").animate({opacity:'0.4'},600);
            })
            $(".go_top").click(function(){
                $(window).scrollTop(0);
            })
        });

        var $win = $(window);
        var $marker = $('.footer_in');
        $win.scroll(function() {
            set_top_go();
        });

        $(window).resize(function() {
            set_top_go();
        });

        function set_top_go(){
            k=$win.scrollTop() + $win.height();
            m=$marker.offset().top-15;
            l1=$('.footer_in').width()+$('.content').offset().left-120;
            if (k >= m) {
                $(".go_top").css("bottom",k-m+'px')
                //$(".go_top").css("left",l1+'px')
            }else{
                $(".go_top").css("bottom",'0px')
                //$(".go_top").css("left",l1+'px')
            }
        }
    </script>
